    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_gosing_ad"
    app:startDestination="@id/fragment_ad_select">

    <fragment
        android:id="@+id/fragment_ad_select"
        android:name="com.moaplanet.gosingadmin.main.submenu.ad.fragment.AdSelectFragment"
        tools:layout="@layout/fragment_ad_select">
        <action
            android:id="@+id/action_fragment_ad_payment"
            app:destination="@id/fragment_ad_payment" />
    </fragment>

    <fragment
        android:id="@+id/fragment_ad_payment"
        android:name="com.moaplanet.gosingadmin.main.submenu.ad.fragment.AdPaymentFragment"
        tools:layout="@layout/fragment_ad_payment" >
        <action
            android:id="@+id/action_fragment_ad_input_pw"
            app:destination="@id/fragment_ad_input_pw" />
    </fragment>

    <fragment
        android:id="@+id/fragment_ad_input_pw"
        android:name="com.moaplanet.gosingadmin.main.submenu.ad.fragment.AdInputPwFragment"
        tools:layout="@layout/fragment_password_input" />

</navigation>