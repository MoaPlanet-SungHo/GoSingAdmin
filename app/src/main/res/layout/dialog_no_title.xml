<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="313.3dp"
    android:layout_height="wrap_content"
    android:background="@drawable/border_rect_7_7_7_7_0dp_trans_ffffff"
    android:minWidth="313.3dp"
    android:paddingBottom="20dp">

    <TextView
        android:id="@+id/tv_no_title_content"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="27dp"
        android:fontFamily="@font/font_gd_15"
        android:lineSpacingExtra="5.3dp"
        android:textColor="@color/color_000000"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpUsage"
        tools:text="다이얼로그 내용" />

    <TextView
        android:id="@+id/tv_no_title_ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/font_gd_15"
        android:paddingStart="20dp"
        android:paddingEnd="27dp"
        android:text="@string/dialog_no_title_done"
        android:textColor="@color/color_4300ff"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_no_title_content"
        tools:ignore="SpUsage" />

    <TextView
        android:id="@+id/tv_dialog_no_title_no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/font_gd_15"
        android:paddingStart="0dp"
        android:paddingEnd="20.7dp"
        android:text="@string/dialog_no_title_no"
        android:textColor="@color/color_4300ff"
        android:textSize="14dp"
        app:layout_constraintEnd_toStartOf="@id/tv_no_title_ok"
        app:layout_constraintTop_toBottomOf="@id/tv_no_title_content"
        tools:ignore="SpUsage" />

</androidx.constraintlayout.widget.ConstraintLayout>