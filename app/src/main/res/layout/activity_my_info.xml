<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_ffffff"
    tools:context=".main.slide_menu.information.MyInfoActivity">

    <com.moaplanet.gosingadmin.common.view.CommonTitleBar
        android:id="@+id/common_activity_my_info_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="@string/activity_my_info_title" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/common_activity_my_info_title">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_marginStart="20dp">

            <TextView
                android:id="@+id/tv_activity_my_info_email_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/font_gd_14"
                android:lineSpacingExtra="4.3dp"
                android:text="@string/activity_my_info_email"
                android:textColor="@color/color_000000"
                android:textSize="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="SpUsage,UnusedAttribute" />

            <TextView
                android:id="@+id/tv_activity_my_info_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40.7dp"
                android:fontFamily="@font/font_gd_15"
                android:textColor="@color/color_000000"
                android:textSize="13dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/tv_activity_my_info_email_title"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="SpUsage"
                tools:text="asdasd@asdasd.com" />

            <View
                android:layout_width="0dp"
                android:layout_height="@dimen/under_line_height"
                android:layout_marginEnd="20dp"
                android:background="@color/color_f5f5f5"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_marginStart="20dp">

            <TextView
                android:id="@+id/tv_activity_my_info_pw_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/font_gd_14"
                android:lineSpacingExtra="4.3dp"
                android:text="@string/activity_my_info_pw"
                android:textColor="@color/color_000000"
                android:textSize="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="SpUsage,UnusedAttribute" />

            <TextView
                android:id="@+id/tv_activity_my_info_pw"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:fontFamily="@font/font_gd_15"
                android:textColor="@color/color_000000"
                android:textSize="13dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/tv_activity_my_info_pw_title"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="SpUsage"
                tools:text="********" />

            <TextView
                android:id="@+id/tv_activity_my_info_pw_change"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:drawableEnd="@drawable/right_arrow_4300ff"
                android:fontFamily="@font/font_gd_13"
                android:gravity="center_vertical"
                android:paddingStart="0dp"
                android:paddingEnd="20dp"
                android:text="변경"
                android:textColor="@color/color_4300ff"
                android:textSize="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="SpUsage" />

            <View
                android:layout_width="0dp"
                android:layout_height="@dimen/under_line_height"
                android:layout_marginEnd="20dp"
                android:background="@color/color_f5f5f5"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_marginStart="20dp">

            <TextView
                android:id="@+id/tv_activity_my_info_phone_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/font_gd_14"
                android:lineSpacingExtra="4.3dp"
                android:text="@string/activity_my_info_phone"
                android:textColor="@color/color_000000"
                android:textSize="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="SpUsage,UnusedAttribute" />

            <TextView
                android:id="@+id/tv_activity_my_info_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40.7dp"
                android:fontFamily="@font/font_gd_15"
                android:textColor="@color/color_000000"
                android:textSize="13dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/tv_activity_my_info_phone_title"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="SpUsage"
                tools:text="010-123-4545" />

            <TextView
                android:id="@+id/tv_activity_my_info_phone_number_change"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:drawableEnd="@drawable/right_arrow_4300ff"
                android:fontFamily="@font/font_gd_13"
                android:gravity="center_vertical"
                android:paddingStart="0dp"
                android:paddingEnd="20dp"
                android:text="변경"
                android:textColor="@color/color_4300ff"
                android:textSize="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="SpUsage" />

            <View
                android:layout_width="0dp"
                android:layout_height="@dimen/under_line_height"
                android:layout_marginEnd="20dp"
                android:background="@color/color_f5f5f5"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="19.3dp"
            android:fontFamily="@font/font_gd_13"
            android:lineSpacingExtra="4dp"
            android:text="@string/activity_my_info_send_msg"
            android:textColor="@color/color_aaaaaa"
            android:textSize="11dp"
            tools:ignore="SpUsage" />

    </LinearLayout>

