<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="310dp"
    android:layout_height="wrap_content"
    android:background="#4d000000"
    android:minWidth="310dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="310dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_food_order_history"
        android:paddingBottom="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_dialog_food_order_close"
            android:layout_width="60dp"
            android:layout_height="24dp"
            app:layout_constraintBottom_toBottomOf="@id/tv_dialog_food_order_title"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/tv_dialog_food_order_title">

            <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginStart="14dp"
                android:contentDescription="@string/dialog_food_order_close_description"
                android:scaleType="fitXY"
                android:src="@drawable/ic_close"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/tv_dialog_food_order_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:fontFamily="@font/font_gd_15"
            android:text="@string/dialog_food_order_title"
            android:textColor="@color/color_000000"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpUsage" />

        <View
            android:id="@+id/view_dialog_food_order_underline"
            android:layout_width="0dp"
            android:layout_height="@dimen/under_line_height"
            android:layout_marginStart="17dp"
            android:layout_marginTop="15.7dp"
            android:layout_marginEnd="17dp"
            android:background="@color/color_f5f5f5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_food_order_title" />

        <TextView
            android:id="@+id/tv_dialog_food_order_history_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30.3dp"
            android:fontFamily="@font/font_gd_13"
            android:text="@string/dialog_food_order_history"
            android:textColor="@color/color_000000"
            android:textSize="14dp"
            app:layout_constraintStart_toStartOf="@id/view_dialog_food_order_underline"
            app:layout_constraintTop_toBottomOf="@id/view_dialog_food_order_underline"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/tv_dialog_food_order_history"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:fontFamily="@font/font_gd_17"
            android:lineSpacingExtra="5dp"
            android:text="깐풍기 1개, 찹쌀 탕수육1개\n고추잡채 1개, 하바네로 닭강정 1개"
            android:textAlignment="textEnd"
            android:textColor="@color/color_000000"
            android:textSize="14dp"
            app:layout_constraintEnd_toEndOf="@id/view_dialog_food_order_underline"
            app:layout_constraintStart_toEndOf="@id/tv_dialog_food_order_history_title"
            app:layout_constraintTop_toTopOf="@id/tv_dialog_food_order_history_title"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/tv_dialog_food_order_time_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10.7dp"
            android:fontFamily="@font/font_gd_13"
            android:text="@string/dialog_food_order_time"
            android:textColor="@color/color_000000"
            android:textSize="14dp"
            app:layout_constraintStart_toStartOf="@id/view_dialog_food_order_underline"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_food_order_history"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/tv_dialog_food_order_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/font_gd_17"
            android:text="2019년 09월 05일 19시 30분"
            android:textColor="@color/color_000000"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@id/tv_dialog_food_order_time_title"
            app:layout_constraintEnd_toEndOf="@id/view_dialog_food_order_underline"
            app:layout_constraintTop_toTopOf="@id/tv_dialog_food_order_time_title"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/tv_dialog_food_order_price_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/font_gd_13"
            android:text="@string/dialog_food_order_price"
            android:textColor="@color/color_000000"
            android:textSize="14dp"
            app:layout_constraintStart_toStartOf="@id/view_dialog_food_order_underline"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_food_order_time_title"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/tv_dialog_food_order_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/font_gd_17"
            android:lineSpacingExtra="5dp"
            android:text="0원"
            android:textColor="@color/color_000000"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@id/tv_dialog_food_order_price_title"
            app:layout_constraintEnd_toEndOf="@id/view_dialog_food_order_underline"
            app:layout_constraintTop_toTopOf="@id/tv_dialog_food_order_price_title"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/tv_dialog_food_order_delivery_price_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/font_gd_13"
            android:lineSpacingExtra="5dp"
            android:text="@string/dialog_food_order_delivery_price"
            android:textColor="@color/color_000000"
            android:textSize="14dp"
            app:layout_constraintStart_toStartOf="@id/view_dialog_food_order_underline"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_food_order_price_title"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/tv_dialog_food_order_delivery_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/font_gd_17"
            android:text="0원"
            android:textColor="@color/color_000000"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@id/tv_dialog_food_order_delivery_price_title"
            app:layout_constraintEnd_toEndOf="@id/view_dialog_food_order_underline"
            app:layout_constraintTop_toTopOf="@id/tv_dialog_food_order_delivery_price_title"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/tv_dialog_food_order_commission_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/font_gd_13"
            android:lineSpacingExtra="5dp"
            android:text="@string/dialog_food_order_commission"
            android:textColor="@color/color_000000"
            android:textSize="14dp"
            app:layout_constraintStart_toStartOf="@id/view_dialog_food_order_underline"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_food_order_delivery_price_title"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/tv_dialog_food_order_commission"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/font_gd_17"
            android:text="0원"
            android:textColor="@color/color_000000"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@id/tv_dialog_food_order_commission_title"
            app:layout_constraintEnd_toEndOf="@id/view_dialog_food_order_underline"
            app:layout_constraintTop_toTopOf="@id/tv_dialog_food_order_commission_title"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/tv_dialog_food_order_total_price_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/font_gd_15"
            android:lineSpacingExtra="5dp"
            android:text="@string/dialog_food_order_total_price"
            android:textColor="@color/color_4300ff"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@id/tv_dialog_food_order_total_price"
            app:layout_constraintStart_toStartOf="@id/view_dialog_food_order_underline"
            app:layout_constraintTop_toTopOf="@id/tv_dialog_food_order_total_price"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/tv_dialog_food_order_total_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/font_gd_17"
            android:text="0원"
            android:textColor="@color/color_4300ff"
            android:textSize="20dp"
            app:layout_constraintEnd_toEndOf="@id/view_dialog_food_order_underline"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_food_order_commission_title"
            tools:ignore="SpUsage" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>