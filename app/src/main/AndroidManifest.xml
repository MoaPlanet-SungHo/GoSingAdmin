<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.moaplanet.gosingadmin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".GoSingAdminApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name=".intro.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".intro.GoSingAdminConfirmPermissionActivity" />
        <activity android:name=".intro.sign_up.SignUpActivity" />
        <activity android:name=".intro.login.LoginActivity" />
        <activity android:name=".main.MainActivity" />
        <activity android:name=".main.submenu.point.activity.PointHistoryActivity" />
        <activity android:name=".main.submenu.notification.NotificationActivity" />
        <activity android:name=".main.submenu.ad.activity.GoSingAdActivity" />
        <activity android:name=".main.submenu.store.StoreActivity" />
        <activity android:name=".main.submenu.review.ReviewActivity" />
        <activity android:name=".main.submenu.eat.EatOrderActivity" />
        <activity android:name=".main.submenu.non_member.NonMemberSaveActivity" />
    </application>

</manifest>